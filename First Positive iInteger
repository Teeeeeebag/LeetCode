


class Solution {
public:
    int firstMissingPositive(int A[], int n) {
        if(n==0)
           return 1;
        for(int i=0; i<n; ++i){
           while(A[i]!=i+1 && A[i]>0 && A[i]<=n&&A[A[i]-1] != A[i]){
              int temp = A[A[i]-1];
              A[A[i]-1] = A[i];
              A[i] = temp;
           }
        }
        int i=0;
        for(; i<n; ++i){
           if(A[i]!=i+1)
              break;
        }
        return i+1;
    }
};
