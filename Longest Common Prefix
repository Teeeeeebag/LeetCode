class Solution {
public:
    string longestCommonPrefix(vector<string> &strs) {
        string prefix;
        if(strs.empty())
           return prefix;
        int cur = 0;
        for(; cur<strs[0].size(); ++cur){
           for(int i=1; i<strs.size(); ++i){
              if(strs[0][cur] != strs[i][cur]){
                 string temp(strs[0].begin(), strs[0].begin()+cur);
                 return temp;
              }
           }
        }
        return strs[0];
    }  
};
